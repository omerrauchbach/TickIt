            <Font size="19.0" />
         </font></Button>
      <Button fx:id="changeButton" layoutX="193.0" layoutY="276.0" mnemonicParsing="false" onAction="#ChangeVacation" prefHeight="56.0" prefWidth="101.0" text="Replace">
