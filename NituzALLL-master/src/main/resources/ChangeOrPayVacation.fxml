<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ChangeOrPayViewController">
   <children>
      <ImageView fitHeight="512.0" fitWidth="698.0" layoutX="-65.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@reka2.png" />
         </image>
      </ImageView>
      <Button fx:id="payButton" layoutX="122.0" layoutY="109.0" mnemonicParsing="false" onAction="#PayCash" prefHeight="56.0" prefWidth="101.0" text="Pay cash">
